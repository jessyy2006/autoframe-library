#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/mco/Projects/Jessica3/autoframe-library/node_modules/dts-bundle-generator/dist/bin/node_modules:/home/mco/Projects/Jessica3/autoframe-library/node_modules/dts-bundle-generator/dist/node_modules:/home/mco/Projects/Jessica3/autoframe-library/node_modules/dts-bundle-generator/node_modules:/home/mco/Projects/Jessica3/autoframe-library/node_modules:/home/mco/Projects/Jessica3/node_modules:/home/mco/Projects/node_modules:/home/mco/node_modules:/home/node_modules:/node_modules:/home/mco/Projects/Jessica3/autoframe-library/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/mco/Projects/Jessica3/autoframe-library/node_modules/dts-bundle-generator/dist/bin/node_modules:/home/mco/Projects/Jessica3/autoframe-library/node_modules/dts-bundle-generator/dist/node_modules:/home/mco/Projects/Jessica3/autoframe-library/node_modules/dts-bundle-generator/node_modules:/home/mco/Projects/Jessica3/autoframe-library/node_modules:/home/mco/Projects/Jessica3/node_modules:/home/mco/Projects/node_modules:/home/mco/node_modules:/home/node_modules:/node_modules:/home/mco/Projects/Jessica3/autoframe-library/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../dts-bundle-generator/dist/bin/dts-bundle-generator.js" "$@"
else
  exec node  "$basedir/../dts-bundle-generator/dist/bin/dts-bundle-generator.js" "$@"
fi
